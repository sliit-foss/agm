const agendaTime8 = "1:00 PM";
const AGMAgendaTitle8 = "End";
const AGMAgendaDescription8 = "End of the AGM 2023/2024";
const eventLink = [
  'https://www.youtube.com/embed/upkXDVJgedU',
  'https://www.youtube.com/embed/upkXDVJgedU',
  'https://www.youtube.com/embed/upkXDVJgedU',
  'https://www.youtube.com/embed/upkXDVJgedU',
  'https://www.youtube.com/embed/upkXDVJgedU',
  'https://www.youtube.com/embed/upkXDVJgedU',
  'https://www.youtube.com/embed/upkXDVJgedU',
  'https://www.youtube.com/embed/upkXDVJgedU',
  'https://www.youtube.com/embed/upkXDVJgedU',
]
const event  = () => {
  const list = [];
  for (let i = 1; i <= 8; i++) {
    console.log(eventLink[i]);
    console.log(`/img/event/${i}.jpg`);
    list.push({
      href: eventLink[i],
      src: `/img/event/${i}.jpg`,
    });
  }
  return list;
}
const gallery = () => {
  const list = [];
  for (let i = 1; i <= 8; i++) {
    list.push({
      src: `/img/gallery/${i}.jpg`,
      title: `Gallery ${i}`,
    });
  }
  return list;
};
